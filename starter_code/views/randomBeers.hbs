
{{#each beers}}
<div class="card img-beers1" style="width: 33.333%">
    <div class="img-beers2">
      <img class="card-img-top" src={{this.image_url}} alt="beers">
    </div>
      <div class="card-body">
        <h4 class="card-title">{{this.name}}</h4>
        <p class="card-text">{{this.tagline}}</p>
        <p class="card-text">{{this.description}}</p>
        <p class="card-text">{{this.food_pairing}}</p>
        <p class="card-text">{{this.brewers_tips}}</p>
      </div>
</div>
{{/each}}